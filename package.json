{
  "name": "",
  "author": "",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "coffee-script": "^1.7.1",
    "coffeeify": "^0.6.0",
    "ecstatic": "0.5.1",
    "jadeify": "^2.1.1",
    "sassify": "^0.5.0",
    "jade": "^1.3.1"
  },
  "devDependencies": {
    "browserify": "^3.46.0",
    "supervisor": "^0.6.0",
    "watchify": "^0.8.3"
  },
  "scripts": {
    "start": "node index.js",
    "prestart": "npm run build",
    "build": "browserify client/index.coffee | uglifyjs -mc > public/browserify.js",
    "watch-client": "watchify client/index.coffee -o public/browserify.js -dv",
    "watch-server": "supervisor -n exit -i public,client -e js,coffee index.js",
    "dev": "npm run watch-client & npm run watch-server"
  },
  "license": "MIT",
  "browserify": {
    "transform": [
      "coffeeify",
      "jadeify",
      "sassify"
    ]
  }
}
